<div class="container my-5">
    <div class="row justify-content-center">
        <div class="col-md-8">
            <div class="card sahdow">
                <div class="card-body">
                    <h2>UserData-Table</h2>
                    <hr>
                    <table class="table striped Sstudent_table">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>Name</th>
                                <th>Email</th>
                                <th>Cell</th>
                                <th>Photo</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% allStudentData.reverse().forEach((student, index) => { %>
                                <tr>
                                    <td><%= index + 1 %> </td>
                                    <td><%= student.name %> </td>
                                    <td><%= student.email %> </td>
                                    <td><%= student.cell %> </td>
                                    <td><img src="/images/userPhoto/<%= student.photo %> " alt=""></td>
                                    <td>
                                        <a class="btn btn-sm btn-info" href="user/single/<%= student.id %>"><i class="fas fa-eye"></i></a>
                                        <a class="btn btn-sm btn-warning" href="user/edit/<%= student.id %>"><i class="fas fa-edit"></i></a>
                                        <a class="btn btn-sm btn-danger delete_btn" href="user/delete/<%= student.id %> "><i class="fas fa-trash"></i></a>
                                    </td>
                                </tr>
                            <% }) %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="card">
                <div class="card-body">
                    <h2>User-Form</h2>
                    <hr>
                    <form action="/user" method="post" class="user_form" enctype="multipart/form-data">
                        <div class="my-3">
                            <label for="">Name</label>
                            <input type="text" name="name" class="form-control">
                        </div>
                        <div class="my-3">
                            <label for="">Email</label>
                            <input type="text" name="email" class="form-control">
                        </div>
                        <div class="my-3">
                            <label for="">Cell</label>
                            <input type="text" name="cell" class="form-control">
                        </div>
                        <div class="my-3">
                            <label for="">Photo</label>
                            <br>
                            <img id="preview_img" src="" alt="">
                            <label for="user_photo_upload"><img src="https://t4.ftcdn.net/jpg/02/17/88/73/360_F_217887350_mDfLv2ootQNeffWXT57VQr8OX7IvZKvB.jpg" alt=""></label>
                            <br>
                            <input id="user_photo_upload" name="user_photo_upload" multiple type="file" class="form-control">
                        </div>
                        <div class="my-3">
                            <label for="">Upload Your CV</label>
                            <input type="file" name="cv" class="form-control">
                        </div>
                        <div class="my-3">
                            <input type="submit" class="btn btn-sm btn-primary w-100">
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>